<% for(var i = 0; i < services.length; i++) { %>
    <button id="<%= services[i].id %>" data-type="top-service"
            style="background: url('<%= services[i].image + services[i].id + ".png" %>')"
            class="<%= services[i].class %>" onmousedown="mDown(this)" onmouseup="mUp(this)"></button>
<% } %>	
